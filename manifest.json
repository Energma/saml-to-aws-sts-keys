{
  "manifest_version": 3,
  "author": "Energma",
  "homepage_url": "https://github.com/Energma/saml-to-cloud-credentials-sts-keys",
  "name": "Energma: SAML to AWS/Aliyun STS Keys Conversion",
  "description": "Generates credentials file with AWS/Aliyun Keys (STS) after logging in to AWS/Aliyun using SSO (SAML 2.0). It uses'assumeRoleWithSAML' API.",
  "version": "1.0",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_icon": "icons/icon_32.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background/script.js"
  },
  "options_page": "options/options.html",
  "permissions": ["webRequest", "storage", "downloads"],
  "host_permissions": ["<all_urls>"]
}
